{
  "name": "g-vet-system",
  "version": "2.0.0",
  "description": "G-VET ASSET & iSTOR SYSTEM - Malaysian Government Official Portal",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "migrate": "node database/migrate.js",
    "seed": "node database/seed.js",
    "reset-db": "npm run migrate && npm run seed",
    "backup": "node scripts/backup.js",
    "restore": "node scripts/restore.js",
    "cleanup-backups": "node scripts/cleanup-backups.js",
    "build": "echo 'No build step required'",
    "prod": "NODE_ENV=production node server.js",
    "test": "echo 'Tests coming soon'",
    "lint": "eslint .",
    "setup": "npm install && npm run migrate && npm run seed && npm start",
    "health": "curl http://localhost:3000/api/health",
    "logs": "tail -f logs/app.log"
  },
  "keywords": [
    "asset-management",
    "inventory",
    "kew-pa",
    "kew-ps",
    "kew-ah",
    "government",
    "malaysia"
  ],
  "author": "Jabatan Perkhidmatan Veterinar Negeri Perak",
  "license": "PROPRIETARY",
  "dependencies": {
    "express": "^4.18.2",
    "bcryptjs": "^2.4.3",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2",
    "mysql2": "^3.6.5",
    "sequelize": "^6.35.2",
    "dotenv": "^16.3.1",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "morgan": "^1.10.0",
    "express-rate-limit": "^7.1.5",
    "multer": "^1.4.5-lts.1",
    "pdfkit": "^0.13.0",
    "exceljs": "^4.4.0",
    "qrcode": "^1.5.3",
    "socket.io": "^4.7.2",
    "nodemailer": "^6.9.7",
    "winston": "^3.11.0",
    "compression": "^1.7.4",
    "express-validator": "^7.0.1",
    "uuid": "^9.0.1",
    "moment": "^2.29.4"
  },
  "devDependencies": {
    "nodemon": "^3.0.2",
    "jest": "^29.7.0",
    "eslint": "^8.56.0",
    "supertest": "^6.3.3"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
}
